										<option value="1">-正常-</option>
										<option value="0">-暂停-</option>
									</select>
								</td>
							</TR>

							
							<tr>
								<td rowspan=2>
								<INPUT class=button id=sButton2 type=submit
														value=" 保存 " name=sButton2>
								</td>
							</tr>
						</TABLE>
						
						
					</TD>
